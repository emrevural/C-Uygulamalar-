#include <stdio.h>
#include <locale.h>
#include <stdlib.h>
main(){
    setlocale(LC_ALL,"Turkish");

    int i,j;
    for(i=2;i<=20;i++){
        for(j=2;j<=(i/j);j++)
            if(!(i%j))
                break;
            if(j>(i/j))
                printf("%d say覺s覺 asal say覺d覺r\n",i);

}
        return 0;

}
